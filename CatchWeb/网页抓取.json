{
	CatchWebSite:{
    Url: "http://www.javbooks.com/index.php?&now_page={1}",
    Param1: "1[TO]3",
    Encode: "UTF-8",
    Proxyurl: "http://127.0.0.1:30000/",
		CatchWebContents:
		[
			{
				SqlTableName:"MovieDetailList",
				RemoveDuplicate:"Title",
				WebContentParts:
				[
					{
						Title:"Title",
            Query: "div.Po_topic_title > a > b"
					},
					{
						Title:"MovieId",
            Query: "div.Po_topic_title > a",
            Attr: "href",
            LastStartWith: "id="
					},
          {
						Title:"DetailHref",
            Query: "div.Po_topic_title > a",
            Attr: "href",
            BeforeAdd: "http://www.javbooks.com/",
            CatchWebSites: [
              {
                Url: "{0}",
                Encode: "UTF-8",
                Proxyurl: "http://127.0.0.1:30000/",
                CatchWebContents:
		            [
			            {
				            SqlTableName:"MovieDetails",
				            RemoveDuplicate:"MovieNo",
				            WebContentParts:
				            [
					            {
						            Title:"MovieNo",
                        Query: "font[color='#A63600']"
					            }
				            ]
			            }
		            ]
              }
            ]
					},
					{
						Title:"thumbnailHref",
            Query: "div.Po_topicCG > a > img",
            Attr: "src"
					},
					{
						Title:"thumbnailImg",
            Query: "div.Po_topicCG > a > img",
            Attr: "src",
            IsImage: true
					}
				]
			}
		]
	},
	MsSqlServerConfig:{
		ConnectionString:"Server=.\\sql2005;Database=JavBt;uid=sa;pwd=123"
	}
}